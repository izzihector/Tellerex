<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>	
	
	
	<record id="sh_message_wizard" model="ir.ui.view">
		<field name="name">sh.message.wizard</field>
		<field name="model">sh.message.wizard</field>
		<field name="arch" type="xml">
			<form string="Message">
				<field name="name"/>
			
				<footer>
					<button string="OK" class="btn-primary" special="cancel" />
				</footer>				
					
				
			</form>
		</field>	
	</record>

	<record id="serial_number_history" model="ir.ui.view">
		<field name="name">serial.number.history</field>
		<field name="model">serial.number.history</field>
		<field name="arch" type="xml">
			<form string="Message">
				<field name="name"/>

				<footer>
					<button string="Close" class="btn-primary" special="cancel" />
				</footer>


			</form>
		</field>
	</record>


		<record id="stand_not_available" model="ir.ui.view">
		<field name="name">stand.not.available</field>
		<field name="model">stand.not.available</field>
		<field name="arch" type="xml">
			<form string="Message">
				<field name="name"/>

				<footer>
					<b>Note </b>:- By Clicking on Yes, this Shipment will be processed without stand.
					<br/>
					<button name = "byepass_stand" type="object" string="Yes" class="btn-primary" />
					OR
					<button string="No" class="oe_link" special="cancel" />
				</footer>


			</form>
		</field>
	</record>

</data>
</odoo>