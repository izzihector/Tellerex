<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_users_form_view_ticl" model="ir.ui.view">
            <field name="name">res.users.form.view.inh</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                 <xpath expr="//page[1]" position="after">
                   <page string="Warehouse">
                    <group>
                    <group>
                        <field name="warehouse_id" style="width: 80%"/>
                    </group>
                    <group>
                        <field name="funding_doc_type" style="width: 100%" invisible="1"/>
                        <field name="funding_doc_number" style="width: 100%" invisible="1"/>
                        <field name="ticl_project_id" style="width: 100%" invisible="1"/>
                    </group>
                    </group>
                    </page>
                </xpath>
            </field>
        </record>
	</data>
</odoo>

