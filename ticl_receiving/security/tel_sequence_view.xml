<?xml version="1.0" encoding="utf-8" ?>
<odoo>
     <data noupdate="1">

	<!-- Sequence Number For receiving -->
		<record id="seq_ticl_receipt_numbers" model="ir.sequence">
		    <field name="name">ticl_receipt_sequences</field>
		    <field name="code">ticl.receipt</field>
		    <field name="prefix">TEL/</field>
		    <field name="padding">5</field>
		</record>

        <record model="ir.sequence" id="seq_ticl_receipt_line_numbers">
            <field name="name">receipts line sequence</field>
            <field name="code">ticl.receipt.log.summary.line</field>
            <field name="active">TRUE</field>
            <field name="prefix"></field>
            <field name="number_increment">1</field>
            <field name="number_next">1000</field>
            <field name="implementation">standard</field>
        </record>

    </data>
</odoo>

