<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_echo_api_shipment" model="ir.cron">
        <field name="name">Shipment; Echo API For POST</field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_ticl_shipment_log"/>
        <field name="code">model.cron_shipment_echo_token()</field>
        <field name="state">code</field>
    </record>

